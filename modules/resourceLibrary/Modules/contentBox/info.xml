<?xml version="1.0" encoding="UTF-8"?>
<resLib type="module">

        <!-- ********   contentBox  ************ -->
        <resource access="general" id="contentBox" name="contentBox/contentBox">
            <description>
                Displays a content entry from the content library
            </description>
			<attributes>
				<attribute name="resourceID" 
							required="true"
							description="The resource ID of the content snippet to add to the page."
							type="resourceOnly"
							resourceType="content"
							resourceField="ID" />
			</attributes>
            <moduleInfo>
                <authorName>Oscar Arevalo</authorName>
                <authorEmail>oarevalo@gmail.com</authorEmail>
                <authorURL>http://www.homeportals.com</authorURL>
                <screenshot/>
            </moduleInfo>
			<api />
        </resource>	
 
</resLib>